local function CopyDict(original) -- Yes, I stole this from DevHub. No, I don't care.
	local copy = {}
	for k, v in pairs(original) do
		if type(v) == "table" then
			v = CopyDict(v)
		end
		copy[k] = v
	end
	return copy
end

